{
    "name": "up-gugu-list",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "获取 up 主的咕咕信息",
    "permissions": [
        "https://space.bilibili.com/*"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
	"content_scripts": [
		{
			"matches": ["https://space.bilibili.com/*"],
			"js": ["index.js"]
		}
	],
    "web_accessible_resources": [
        "/assets/index.dht.js",
        "/assets/chunk.dht.js",
        "/assets/index.dht.css",
        "/assets/index.dht2.css"
    ]
}