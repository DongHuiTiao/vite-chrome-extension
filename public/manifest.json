{
    "name": "test",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "用于测试",
    "permissions": [
        "https://www.baidu.com/*"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["index.js"]
		}
	],
    "web_accessible_resources": [
        "/assets/index.dht.js",
        "/assets/vendor.511a2199.js"
    ]
}