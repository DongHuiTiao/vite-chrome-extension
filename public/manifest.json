{
    "name": "test",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "用于测试",
    "permissions": [
        "https://member.bilibili.com/*"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
	"content_scripts": [
		{
			"matches": ["https://member.bilibili.com/*"],
			"js": ["index.js"]
		}
	],
    "web_accessible_resources": [
        "/assets/index.dht.js",
        "/assets/chunk.dht.js",
        "/assets/index.dht.css",
        "/assets/index.dht2.css"
    ]
}