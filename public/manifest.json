{
    "name": "test",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "用于测试",
    "permissions": [
        "management",
        "https://www.baidu.com/*"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
	"content_scripts": [
		{
			// "matches": ["<all_urls>"],
            "matches": ["https://www.baidu.com/*"],
			"js": ["index.js"]
		}
	],
    "web_accessible_resources": [
        "/assets/index.dht.js",
        "/assets/vendor.1e3d9d58.js"
    ]
}